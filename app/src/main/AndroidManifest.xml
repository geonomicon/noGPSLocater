<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project.rajatsharma.nogpslocater" >

    <uses-sdk
        android:maxSdkVersion="18"
        android:minSdkVersion="15"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <!--
 The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but are recommended.
    -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".CompassActivity"
            android:label="@string/title_activity_compass" >
        </activity>
        <activity
            android:name=".MagnetoActivity"
            android:label="@string/title_activity_magneto" >
        </activity>
        <activity
            android:name=".CellidMap"
            android:label="@string/title_activity_cellid_map" >
        </activity>
        <activity
            android:name=".CellIdActivity"
            android:label="@string/title_activity_cell_id" >
        </activity>
        <activity
            android:name=".GPSdb"
            android:label="@string/title_activity_gpsdb" >
        </activity>
        <activity
            android:name=".TrackingActivity"
            android:label="@string/title_activity_tracking" >
        </activity>
    </application>

</manifest>
